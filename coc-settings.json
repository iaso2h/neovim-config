{
    "clangd.semanticHighlighting": true,
    "clangd.arguments": [
        "--background-index"
    ],
    "cmake.cmakePath": "D:\\CMake\\bin\\cmake.exe",
    "cmake.lsp.enable": false,
    "codeLens.enable": true,
    "coc.preferences.colorSupport": true,
    "coc.preferences.currentFunctionSymbolAutoUpdate": true,
    "coc.preferences.extensionUpdateCheck": "daily",
    "coc.preferences.formatOnType": true,
    "coc.preferences.formatOnSaveFiletypes": [
        "python",
        "markdown",
        //"json",
        "jsonc"
    ],
    "coc.source.around.firstMatch": false,
    "coc.source.file.ignoreHidden": false,
    "diagnostic.checkCurrentLine": true,
    "diagnostic.errorSign": "‚ùå",
    "diagnostic.hintSign": "üí°",
    "diagnostic.infoSign": "üîé",
    "diagnostic.warningSign": "‚ö†Ô∏è",
    "diagnostic.format": "%message [%source]",
    "diagnostic.separateRelatedInformationAsDiagnostics": true,
    "diagnostic.virtualText": true,
    "list.nextKeymap": "<C-n>",
    "list.previousKeymap": "<C-p>",
    "languageserver": {
        "ccls": {
            "command": "ccls",
            "filetypes": [
                "c",
                "cpp",
                "cuda",
                "objc",
                "objcpp"
            ],
            "rootPatterns": [
                ".ccls-root",
                "compile_commands.json"
            ],
            "initializationOptions": {
                "cache": {
                    "directory": ".ccls-cache"
                },
                "client": {
                    "snippetSupport": true
                }
            }
        }
    },
    "markdownlint.config": {
        "default": true,
        "line-length": false
    },
    "snippets.extends": {
        "cpp": [
            "c"
        ],
        "typescript": [
            "javascript"
        ]
    },
    "snippets.shortcut": "V",
    "snippets.enableStatusItem": true,
    "suggest.asciiCharactersOnly": true,
    "suggest.enablePreselect": true,
    "suggest.enablePreview": true,
    "suggest.noselect": false,
    "suggest.timeout": 5000,
    "suggest.triggerAfterInsertEnter": true,
    "suggest.completionItemKindLabels": {
        "keyword": "\uf1de",
        "variable": "\ue79b",
        "value": "\uf89f",
        "operator": "\u03a8",
        "constructor": "\uf0ad",
        "function": "\u0192",
        "reference": "\ufa46",
        "constant": "\uf8fe",
        "method": "\uf09a",
        "struct": "\ufb44",
        "class": "\uf0e8",
        "interface": "\uf417",
        "text": "\ue612",
        "enum": "\uf435",
        "enumMember": "\uf02b",
        "module": "\uf40d",
        "color": "\ue22b",
        "property": "\ue624",
        "field": "\uf9be",
        "unit": "\uf475",
        "event": "\ufacd",
        "file": "\uf723",
        "folder": "\uf114",
        "snippet": "\ue60b",
        "typeParameter": "\uf728",
        "default": "\uf29c"
    },
    "vimlsp.trace.server": "verbose",
    "vimlsp.indexes.projectRootPatterns": [
        ".git",
        ".vim",
        "autoload",
        "plugin"
    ],
    "python.formatting.provider": "black",
    "python.formatting.blackPath": "/home/iaso2h/.local/bin/black",
    "cSpell.enabled": false,
    "cSpell.allowCompoundWords": true,
    "cSpell.userWords": [
        "iaso2h"
    ]
}
