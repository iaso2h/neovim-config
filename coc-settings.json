{
    "cSpell.enabled": false,
    "codeLens.enable": true,
    "coc.preferences.colorSupport": true,
    "coc.preferences.extensionUpdateCheck": "daily",
    "coc.preferences.formatOnType": true,
    "highlight.document.enable": true,
    "highlight.colors.enable": true,
    "highlight.colorNames.enable": true,
    "highlight.disableLanguages": [],
    "coc.preferences.formatOnSaveFiletypes": [
        "python",
        "markdown",
        //"json",
        "jsonc"
    ],
    "coc.source.around.firstMatch": false,
    "coc.source.file.ignoreHidden": false,
    "clangd.semanticHighlighting": true,
    "clangd.arguments": [
        "--background-index"
    ],
    "diagnostic.format": "%message [%source]",
    "diagnostic.virtualText": true,
    "diagnostic.checkCurrentLine": true,
    "diagnostic.separateRelatedInformationAsDiagnostics": true,
    "markdownlint.config": {
        "default": true,
        "line-length": false
    },
    "signature.target": "echo",
    "suggest.timeout": 5000,
    "suggest.asciiCharactersOnly": true,
    "python.formatting.provider": "black",
    "python.formatting.blackPath": "/home/iaso2h/.local/bin/black"
}
